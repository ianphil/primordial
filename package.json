{
  "name": "demigod",
  "version": "0.0.1",
  "description": "Demigod is an automated provisioner for Azure and Docker",
  "main": "server.js",
  "scripts": {
    "prestart": "babel-node scripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:web lint test",
    "open:web": "babel-node svr/server.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 6996",
    "share": "npm-run-all --parallel open:web localtunnel",
    "lint": "esw scripts svr src --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress scripts/testSetup.js \"src/**/*.spec.js\" \"svr/**/*.spec.js\"",
    "test:watch": "npm test -- --watch",
    "generate-mock-data": "babel-node scripts/generateMockData.js",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/fakeApi/db.json --port 6970"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PixelPlayoffs/demigod.git"
  },
  "author": "Tripdubroot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PixelPlayoffs/demigod/issues"
  },
  "homepage": "https://github.com/PixelPlayoffs/demigod#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "open": "0.0.5"
  },
  "devDependencies": {
    "babel-preset-es2015-node": "^6.1.1"
  }
}
